{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/main/server/index.js",
  "scripts": {
    "docker:up": "docker-compose up -d --remove-orphans",
    "docker:down": "docker-compose down",
    "docker:clear": "docker-compose down && docker image prune -a -f && npm run docker:delete:volumes",
    "docker:delete:volumes": "docker volume prune -f",
    "build:docker:dev": "rm -rf docker-compose.yml && cat docker/dev/docker-compose.yml >> docker-compose.yml",
    "build:docker:test": "rm -rf docker-compose.yml && cat docker/test/docker-compose.yml >> docker-compose.yml",
    "build:script:dev": "chmod +x ./scripts/dev/postgres/init.sh",
    "build:script:test": "chmod +x ./scripts/test/postgres/init.sh",
    "prerun:docker:dev": "npm run docker:clear || true",
    "prerun:docker:test": "npm run docker:clear || true",
    "run:docker:dev": "npm run build:script:dev && npm run build:docker:dev",
    "run:docker:test": "npm run build:script:test && npm run build:docker:test",
    "postrun:docker:dev": "npm run docker:up",
    "postrun:docker:test": "npm run docker:up",
    "build:dev": "rm -rf .env && cat ./env/dev/.env >> .env",
    "build:test": "rm -rf .env && cat ./env/test/.env >> .env",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "start": "node dist/src/main/server/index.js",
    "start:queue": "node dist/src/main/broker/index.js",
    "start:dev": "npm run build && npm run start",
    "start:queue:dev": "npm run build && npm run start:queue",
    "prestart:docker:dev:up": "npm run build:dev",
    "start:docker:dev:up": "npm run run:docker:dev",
    "pretest": "npm run build:test && npm run run:docker:test",
    "test": "jest --clearCache ; jest --colors --coverage --verbose --detectOpenHandles --config jest.config.js",
    "posttest": "npm run docker:clear",
    "terraform:init": "docker-compose -f deploy/docker-compose.yml run --rm terraform init",
    "terraform:plan": "docker-compose -f deploy/docker-compose.yml run --rm terraform plan"
  },
  "keywords": [],
  "author": "Gabriel Ferrari Tarallo Ferraz",
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "5.5.9",
    "@types/jest": "~27.4.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.0.3",
    "@types/nodemailer": "^6.4.6",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "cpf-cnpj-validator": "^1.0.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "faker": "5.5.3",
    "jest": "~27.5.1",
    "prettier": "^2.7.1",
    "puppeteer": "18.1.0",
    "ssn": "^1.0.3",
    "supertest": "^6.3.2",
    "ts-jest": "~27.1.4",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "amqplib": "^0.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "uuid": "^9.0.0"
  },
  "license": "MIT"
}
